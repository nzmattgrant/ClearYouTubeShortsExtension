{
  "manifest_version": 3,
  "name": "Delete YouTube Shorts From History",
  "version": "1.1",
  "description": "Adds buttons into YouTube history page to clear/delete short rows",
  "icons": {
    "128": "DeleteShortsIcon128.png",
    "48": "DeleteShortsIcon48.png",
    "16": "DeleteShortsIcon16.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
